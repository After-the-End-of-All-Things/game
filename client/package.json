    "marked": "^7.0.5",
    "ngx-tippy-wrapper": "^6.1.0",
    "ngx-webstorage": "^12.0.0",
    "rollbar": "^2.26.1",
    "rss-parser": "^3.13.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.2",
    "tslib": "^2.3.0",
